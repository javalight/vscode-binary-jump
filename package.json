{
  "name": "binaryjump",
  "displayName": "Binary Jump",
  "publisher": "vipinbathaw",
  "version": "0.1.0",
  "description": "Extension for Visual Studio Code that helps you jump at center of your current position to the start/end of line based on direction you choose to jump.",
  "main": "./extension",
  "engines": {
    "vscode": "^1.30.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "commands": [
      {
        "command": "binaryJump.binaryLeftJump",
        "title": "Binary Left Jump"
      },
      {
        "command": "binaryJump.binaryRightJump",
        "title": "Binary Right Jump"
      }
    ],
    "keybindings": [
      {
        "command": "binaryJump.binaryLeftJump",
        "key": "ctrl+meta+alt+left",
        "mac": "ctrl+cmd+alt+left",
        "when": "editorTextFocus"
      },
      {
        "command": "binaryJump.binaryRightJump",
        "key": "ctrl+meta+alt+right",
        "mac": "ctrl+cmd+alt+right",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "test": "node ./node_modules/vscode/bin/test",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "vscode": "^1.1.25",
    "typescript": "^3.1.4",
    "eslint": "^4.11.0",
    "@types/node": "^8.10.25",
    "@types/mocha": "^2.2.42"
  },
  "author": "Vipin Bathaw",
  "license": "MIT"
}
